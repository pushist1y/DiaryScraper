      "from": "../api/DiaryScraperCore/bin/dist/",
      "to": "api/DiaryScraperCore/bin/dist/",
      "filter": [
        "**/*"
      ]
    },
    "mac": {
      "category": "diary-scraper"
    },
    "linux": {
      "category": "diary-scraper",
      "target": [
        "dir",
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
