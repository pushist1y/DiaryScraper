{
  "name": "diary-scraper",
  "version": "0.1.1",
  "title": "Скачать дневник",
  "description": "Scraper for diary.ru blogs",
  "author": {
    "name": "Anton Evseev",
    "email": "mister.p.sh@gmail.com",
    "url": "http://terribles.ru"
  },
  "repository": "https://github.com/pushist1y/DiaryScraper",
  "main": "main.js",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.45.4"
  },
  "dependencies": {
    "angular": "^1.6.6",
    "jquery": "3.2.1",
    "bootstrap": "4.0.0-beta.2",
    "popper.js": "1.12.3",
    "angularjs-datepicker": "^2.1.23"
  },
  "scripts": {
    "start": "electron .",
    "dist": "build"
  },
  "build": {
    "appId": "diary-scraper",
    "directories": {
      "buildResources": "../../assets",
      "output": "../../dist"
    },
    "extraResources": {
      "from": "../api/DiaryScraperCore/bin/dist/",
      "to": "api/DiaryScraperCore/bin/dist/",
      "filter": [
        "**/*"
      ]
    },
    "mac": {
      "category": "diary-scraper"
    },
    "linux": {
      "category": "diary-scraper",
      "target": [
        "dir",
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
  }
}
